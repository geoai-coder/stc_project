{
    "张家界": [
        {
            "text": "张家界景区魅力的关键所在于张家界的山型地貌的奇特，属于罕见的石英砂岩峰林，为世界独有，不管季节怎么变，张家界奇特的山型地貌是永远不会改变的。",
            "spo": [
                "张家界",
                "地貌",
                "石英砂岩峰林"
            ],
            "T": null,
            "L": "张家界"
        },
        {
            "text": "张家界地貌形成于距今3亿8千万年前的泥盆纪的砂岩峰林地貌，是娃娃鱼的主要原产地，拥有着娃娃鱼繁衍生息得天独厚的生态环境。",
            "spo": [
                "张家界",
                "地貌",
                "砂岩峰林地貌"
            ],
            "T": "距今3亿8千万年前的泥盆纪",
            "L": "张家界"
        },
        {
            "text": "地质气候：张家界市地貌构造复杂，主要有山地、岩溶、丘陵、岗地和平原等，山地面积占总面积的76%，其中最具特色的是石英砂岩峰林地貌，为世界罕见。",
            "spo": [
                "张家界",
                "地貌",
                "砂岩峰林地貌"
            ],
            "T": null,
            "L": "张家界"
        },
        {
            "text": "张家界地貌是以侵蚀构造为主导作用，由石英砂岩构成的砂岩峰林地貌。",
            "spo": [
                "张家界",
                "地貌",
                "砂岩峰林地貌"
            ],
            "T": null,
            "L": "张家界"
        },
        {
            "text": "砂岩峰林地貌：湖南省武陵源景区的张家界是典型的砂岩峰林地貌，又称为张家界地貌。",
            "spo": [
                "张家界",
                "地貌",
                "砂岩峰林地貌"
            ],
            "T": null,
            "L": "湖南省"
        }
    ],
    "黄土高原": [
        {
            "text": "在大约800万年的时候，黄土高原湖水终于干枯，黄土被抬出地面，而渐渐的成了高原。也就成了今黄土高坡的地貌。",
            "spo": [
                "黄土高原",
                "地貌",
                "黄土高坡"
            ],
            "T": "大约800万年",
            "L": "黄土高原"
        },
        {
            "text": "分布于山西河曲、保德、兴县、临县等黄河沿岸，以及陕西绥德、米脂、佳县、清涧、神木、府谷等地的饰状丘陵是黄土高原最典型的丘陵地区",
            "spo": [
                "黄土高原",
                "地貌",
                "丘陵"
            ],
            "T": null,
            "L": "山西河曲、保德、兴县、临县等黄河沿岸以及陕西绥德、米脂、佳县、清涧、神木、府谷等地"
        }
    ],
    "喜马拉雅山": [
        {
            "text": "喜马拉雅山山系两侧地势不对称。北侧为地势开阔的高原面和若干宽谷盆地，大部分地区在海拔4500—5200米间，地势向北和向东倾斜。南侧，地势急剧下降到海拔3500—3000米以下，呈现出雄伟壮观的高山深谷地貌。",
            "spo": [
                "喜马拉雅山",
                "地貌",
                "高山深谷"
            ],
            "T": null,
            "L": "喜马拉雅山"
        },
        {
            "text": "喜马拉雅山山区内许多地区属于典型的高原峡谷地貌，具有明显的海拔高、坡度大、切割深等特点。",
            "spo": [
                "喜马拉雅山",
                "地貌",
                "高原峡谷"
            ],
            "T": null,
            "L": "喜马拉雅山"
        }
    ],
    "塔里木盆地": [
        {
            "text": "塔里木盆地是大型封闭性山间盆地，地质构造上是周围被许多深大断裂所限制的稳定地块。盆地地貌呈环状分布，边缘是与山地连接的砾石戈壁，中心是辽阔沙漠，边缘和沙漠间是冲积扇和冲积平原，并有绿洲分布。",
            "spo": [
                "塔里木盆地",
                "地貌",
                "呈环状分布"
            ],
            "T": null,
            "L": "塔里木盆地"
        }
    ],
    "湄公河三角洲": [
        {
            "text": "湄公河下流及其9条叉道流入南海时所形成的冲积平原，称为湄公河三角洲，是越南第一大平原，面积约4.4万平方公里(其中1/5属于柬埔寨)。",
            "spo": [
                "湄公河三角洲",
                "地貌",
                "平原"
            ],
            "T": null,
            "L": "越南"
        }]
}